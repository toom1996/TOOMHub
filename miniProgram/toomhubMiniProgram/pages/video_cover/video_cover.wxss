.box{
  height: 100vh;
  display: flex;
  flex-direction: column;
  background-color: white;
  margin: 20rpx;
  padding: 20rpx;
  border-radius: 20rpx;
} 
.uploadWrap{
  height: calc(50vh - 88rpx);
  overflow-y: scroll;
}
.upload_Item{
  flex: 1;
  } 

.upload_Item_img{ 
  width: 100%; 
  /* height: auto; */
  /* object-fit: cover; */
  height: 160rpx;
}
.checked {
  background-color: black;
  width: 100%;
  /* height: 160rpx; */
  opacity:0.4;
}
.checkedIcon {
  position: absolute;
  top: 0;
  left:0; right:0; top:0; bottom:0;
  margin:auto;
}