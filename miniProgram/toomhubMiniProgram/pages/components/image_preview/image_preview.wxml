<view class="canui-head-box" style="top:{{navTop}}px;">
    <navigator class='icon-back' open-type="navigateBack" hover-class="none"></navigator>
    <navigator class='icon-home' open-type="navigateBack" hover-class="none"></navigator>
</view>

<swiper class="box" indicator-dots="{{indicatorDots}}"
      autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" current="{{ index }}">
      <block wx:for="{{image}}" wx:key="*this">
        <swiper-item>
          <view class="swiper-item"><image bindtap="imageClickHandle" src="{{item}}" mode="widthFix"></image></view>
        </swiper-item>
      </block>
    </swiper>
